        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox spacing="5.0">
               <children>
                  <Label text="始发站" />
                  <TextField />
                  <Button mnemonicParsing="false" text="&lt;&gt;" />
                  <Label text="到达站" />
                  <TextField />
                  <Label text="出发日期" />
                  <DatePicker />
                  <Button mnemonicParsing="false" text="查询" />
               </children>
               <VBox.margin>
                  <Insets bottom="2.0" left="5.0" right="2.0" top="2.0" />
               </VBox.margin>
            </HBox>
            <TableView prefHeight="200.0" prefWidth="200.0">
              <columns>
